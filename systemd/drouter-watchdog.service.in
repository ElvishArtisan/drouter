[Unit]
Description=DRouter Watchdog Monitor
After=network.target remote-fs.target nss-lookup.target
Documentation=man:drouter.conf(5)

[Service]
Type=simple
ExecStart=@prefix@/sbin/drouterdogd
TimeoutStopSec=10
PrivateTmp=true
Restart=always

[Install]
WantedBy=multi-user.target
